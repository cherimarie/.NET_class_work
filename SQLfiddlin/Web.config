<configuration>

    <connectionStrings>
        <add name="CommunityAssistConnectionString1" connectionString="Data Source=localhost;Initial Catalog=CommunityAssist;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    <!--you can have multiple connection strings, for multiple databases-->
    <!--your databases exist independently of projects. they just hang out in server explorer waiting to be connected up.-->
    </connectionStrings>
    <system.web>
      <compilation debug="false" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>

</configuration>
